  C :MSTORE(SP - 1)
  D :MSTORE(SP - 2)
  E :MSTORE(SP - 3)
  B :MSTORE(SP - 4)
  18446744069414584320n => A
  137438953472n => E
  E :MSTORE(SP)
  0 => D
  A => E
  4294967296n => B
  ${E / B} => A
  ${E % B} => C
  E:ARITH
  $ => E :MLOAD(SP)
  A :MSTORE(SP)
  0 => D
  4294967296n => B
  ${E / B} => A
  ${E % B} => C
  E:ARITH
  A => E
  32 => B
  0 => D
  ${E / B} => A
  ${E % B} => C
  E:ARITH
  $ => A :MLOAD(SP)
  C => E
;;NEED_INCLUDE: 2-exp
  zkPC + 2 => RR
    :JMP(@two_power + E)
  A => E
  0 => D
  ${E / B} => A
  ${E % B} => C
  E :ARITH
  0 => C
  4294967296n => B
